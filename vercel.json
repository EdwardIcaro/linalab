{
  "buildCommand": "cd backend && pnpm run build",
  "outputDirectory": "backend/dist",
  "devCommand": "cd backend && pnpm run dev",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/DESKTOPV2/index.html"
    }
  ]
}
